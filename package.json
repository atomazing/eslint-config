{
	"name": "@atomazing-org/eslint-config",
	"version": "2.0.0",
	"type": "module",
	"license": "MIT",
	"author": "whoisYeshua + MarkSinD",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/atomazing/eslint-config.git"
	},
	"bugs": {
		"url": "https://github.com/atomazing/eslint-config.git"
	},
	"homepage": "https://github.com/atomazing/eslint-config.git",
	"keywords": [
		"eslint",
		"atomazing"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"exports": "./index.js",
	"files": [
		"react.js",
		"unicorn.js",
		"index.js",
		"base",
		"CHANGELOG.md"
	],
	"scripts": {
		"format": "npm run format:prettier",
		"format:prettier": "npm run lint:prettier -- --write",
		"lint": "npm run lint:prettier",
		"lint:prettier": "prettier \"**/*.{json,js,ts,tsx}\" --check",
		"clean": "rimraf node_modules",
		"debug:prettier": "prettier \"./index.js\" --loglevel debug",
		"semantic-release:local-check": "semantic-release --dry-run --no-ci",
		"prepare": "husky"
	},
	"dependencies": {
		"@eslint/js": "^9.24.0",
		"@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
		"typescript-eslint": "^8.29.1",
		"globals": "^16.0.0",
		"eslint": "^9.24.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-import-resolver-typescript": "^4.3.2",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.19",
		"eslint-plugin-unicorn": "^58.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"conventional-changelog-conventionalcommits": "^7.0.2",
		"eslint": "^9.24.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.3",
		"rimraf": "^5.0.9",
		"semantic-release": "^23.1.1"
	},
	"packageManager": "pnpm@9.1.2",
	"peerDependencies": {
		"eslint": ">=9.24"
	},
	"engines": {
		"node": ">=18.18"
	}
}
